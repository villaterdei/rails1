<p id="notice"><%= notice %></p>

<h1>Tipos Datos</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_datos.each do |tipos_dato| %>
      <tr>
        <td><%= tipos_dato.date %></td>
        <td><%= tipos_dato.time %></td>
        <td><%= tipos_dato.text %></td>
        <td><%= link_to 'Show', tipos_dato %></td>
        <td><%= link_to 'Edit', edit_tipos_dato_path(tipos_dato) %></td>
        <td><%= link_to 'Destroy', tipos_dato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipos Dato', new_tipos_dato_path %>
